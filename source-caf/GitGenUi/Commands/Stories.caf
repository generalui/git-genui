import &StandardImport, &CommandsLib

description: "" list all open stories
run: (options) ->
  ensureTrackerConfigured()
  .then -> &Tracker.tracker.stories
  .then (stories) ->
    prompt = ->
      &GitGenUiPromptFor.story stories, "" Select a story to update:
      .then (story) ->
        if story.id
          EditStoryMenu story, exitPrompt: "back to stories"
          .then prompt
    prompt()

  .then -> null

