test 'getCommitComment' ->
  expect getCommitComment {}
    generatedCommitMessage = "my-commit-message"
    storyState =             "finished"
    branch =                 "feature/fun"
    commit =                 "abc123"
    coauthors =              "William"

  .toMatch
    ///
      #{storyState}
      (.|\n)*
      #{commit}
      (.|\n)* #{generatedCommitMessage}
      (.|\n)*
      git-genui
      (.|\n)* v \d+\. \d+\. \d+

