import &StandardImport, &Config

class Tracker extends BaseClass

  @classGetter
    projectId:    -> config.tracker?.projectId
    tracker:      -> &Trackers[config.tracker?.name]

  @getStories:    => @tracker.getStories @projectId
  @getMembers:    => @tracker.getMembers @projectId
  @openInBrowser: => @tracker.openInBrowser @projectId
