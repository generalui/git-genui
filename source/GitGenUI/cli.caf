import &ArtStandardLib
fs = &fsExtra

run: ->
  serve = -> log :serving

  &Config.load()
  .then ->
    yargs = &yargs


    each {description, run}, command in &CommandsV2.modules
      yargs = yargs.command
        dashCase command
        description
        run

    yargs
    .option
      :verbose
      alias:        :v
      type:         :boolean
      description:  "" Run with verbose logging

    .argv