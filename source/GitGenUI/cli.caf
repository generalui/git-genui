import &ArtStandardLib
fs = &fsExtra


run: ->
  &Config.load()
  .tap (config) -> &Tracker.tracker.init config
  .then ->
    commandsMap = object m from &CommandsV2.modules when m.description is String
    require :@art-suite/cli
    .start
      commands: object {run} in commandsMap with run
      help: commands: object m in commandsMap
        merge m, options:
          verbose: "" run with verbose logging
