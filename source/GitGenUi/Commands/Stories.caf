import &StandardImport, &InquirerPlus, &CommandsLib

description: "" list all open stories
run:
  (options) ->
    ensureTrackerConfigured()
    .then -> &Tracker.tracker.stories
    .then (stories) ->
      each story in stories
        log "  " + tracker.formatStory story, true
